@model Gosocket.Dian.Web.Models.RadianApproved.RadianApprovedViewModel
@using Gosocket.Dian.Domain.Common;
@using Newtonsoft.Json;
@{var defaultHtmlAttr = new Dictionary<string, object> { { "class", "form-control input-general" }, { "readonly", "true" } };}
@{
    int ContributorId = Model.ContributorId;
    int RadianOperationMode = Model.Contributor.RadianOperationModeId;
    string RadianContributorType = Model.RadianTestSetResult.ContributorTypeId;
    string parametro = Model.RadianContributorTypeId.ToString();
    string participantType = ((RadianContributorType)(Enum.Parse(typeof(RadianContributorType), parametro))).GetDescription();

    ViewBag.title = $"Set de pruebas - {participantType}";
    var status = "";
    switch (Model.RadianTestSetResult.Status)
    {
        case (int)TestSetStatus.InProcess:
            status = "En Proceso";
            break;
        case (int)TestSetStatus.Accepted:
            status = "Aceptado";
            break;
        case (int)TestSetStatus.Rejected:
            status = "Rechazado";
            break;

    }
}


<div class="page-content radian-view margin-20">
    <div class="title-container">
        <p class="title-view">
            @ViewBag.title
        </p>
        <div class="underline-title"></div>
        @if (Model.RadianState == "Habilitado")
        {
            using (Html.BeginForm("GetFactorOperationMode", "RadianApproved", FormMethod.Post))
            {
                @Html.HiddenFor(m => m.Nit);
                @Html.HiddenFor(m => m.Name);
                @Html.HiddenFor(m => m.BusinessName);
                @Html.HiddenFor(m => m.Email);
                @Html.HiddenFor(m => m.Step);
                @Html.HiddenFor(m => m.Nit);
                @Html.HiddenFor(m => m.FilesRequires);
                @Html.HiddenFor(m => m.Customers);
                @Html.HiddenFor(m => m.RadianContributorTypeId);
                @Html.HiddenFor(m => m.ContributorId);
                @Html.HiddenFor(m => m.SoftwareType);

                <button type="submit" class="back-button"><i class="fa fa-chevron-left"></i> Volver</button>
            }
        }
        else
        {
            <div class="back-button"><a href="@Url.Action("Index", "RadianApproved", new { ContributorId, RadianOperationMode, RadianContributorType })"><i class="fa fa-chevron-left"></i> Volver</a></div>
        }
