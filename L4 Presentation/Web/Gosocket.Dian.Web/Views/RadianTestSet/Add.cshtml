@using Gosocket.Dian.Web.Utils;
@model Gosocket.Dian.Web.Models.RadianTestSetViewModel

@{
    /**/

    ViewBag.Title = "Agregar Set de Pruebas";
}

<div class="page-content margin-15">
    <div class="row margin-horizontal-0 margin-top-50">
        <div class="panel">


            <div class="panel-body padding-top-20">
                <div class="row margin-horizontal-0">
                    <div id="panel-form" data-target="#panel-form" data-toggle="panel-overlay" class="panel panel-bordered-dian margin-bottom-0">
                        @using (Html.BeginForm("Add", "RadianTestSet", FormMethod.Post, new { id = "add-testset-form" }))
                        {
                            @Html.Partial("_form")
                        <div class="text-left">
                            <button type="submit" class="btn btn-primary btn-success btn-radian-success">Guardar</button>
                            <a href="@Url.Action("Index", "RadianTestSet")" class="btn btn-secondary btn-radian-default">Volver</a>
                        </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



@section scripts
{
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Content/js/Radian/RadianTestSet.js"></script>

    <style type="text/css">
        .claseLabel {
            /* top: 10px;  */
            position: relative;
            height: 24px;
        }
    </style>
}
